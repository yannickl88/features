{
    "name":              "yannickl88/features-bundle",
    "description":       "Symfony bundle for managing feature tags",
    "license":           "MIT",
    "type":              "symfony-bundle",
    "keywords": [
        "Symfony",
        "Features",
        "Bundle"
    ],
    "authors": [
        {
            "name":     "Yannick de Lange",
            "email":    "yannickl88@gmail.com",
            "homepage": "http://yannickl88.nl/"
        }
    ],
    "require": {
        "php":                          "^8.1",
        "symfony/config":               "^5.4||^6.0",
        "symfony/dependency-injection": "^5.4||^6.0",
        "symfony/finder":               "^5.4||^6.0",
        "symfony/framework-bundle":     "^5.4||^6.0",
        "symfony/http-kernel":          "^5.4||^6.0"
    },
    "require-dev": {
        "phpspec/prophecy-phpunit": "^2.0",
        "phpunit/phpunit":          "^9.5.8",
        "symfony/twig-bundle":      "^5.4||^6.0",
        "symfony/yaml":             "^5.4||^6.0",
        "twig/twig":                "^3.4.3"
    },
    "suggest": {
        "twig/twig": "Allows use of features in twig templates through the FeaturesExtension"
    },
    "minimum-stability": "dev",
    "prefer-stable":     true,
    "autoload": {
        "psr-4": {
            "Yannickl88\\FeaturesBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yannickl88\\FeaturesBundle\\": "test/"
        }
    },
    "archive": {
        "exclude": [
            "/test"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
